{{#each adventures as |adventure|}}

<span id=class="container not-complete form-inline">
  <div>{{adventure.title}}  {{adventure.place}}</div>
  <div> {{adventure.notes}} </div>
  <button id="{{adventure._id}}-checkbox" class="btn btn-danger"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span></button>
  <button id="{{adventure._id}}-edit" class="btn btn-danger"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button>
  <button id="{{adventure._id}}-delete" class="btn btn-danger"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></button>
</span>

{{/each}}
