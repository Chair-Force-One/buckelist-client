{{#each adventures as |adventure|}}

  {{#if adventure.checked}}
    //If done
    <div class="container complete">
      <h3> <small>{{adventure.title}}</small></h3>
      <h4> {{adventure.place}} </h4>
      <p> {{adventure.notes}} </p>>
    </div>

  {{else}}
    <div class="container incomplete">
      <h3> <small>{{adventure.title}}</small></h3>
      <h4> {{adventure.place}} </h4>
      <p> {{adventure.notes}} </p>>
    </div>

  {{/if}}

{{/each}}
