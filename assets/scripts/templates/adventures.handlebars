<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Location</th>
      <th scope="col">Notes</th>
      <th scope="col">Complete</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody id="list-table">
    {{#each adventures as |adventure|}}
        <tr>
          <th scope="row">{{adventure.title}}</th>
          <td>{{adventure.place}}</td>
          <td>{{adventure.notes}}</td>
          <td><button id="{{adventure._id}}-checkbox" class="btn btn-danger"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span></button></td>
          <td><button id="{{adventure._id}}-edit" class="btn btn-danger"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button></td>
          <td><button id="{{adventure._id}}-delete" class="btn btn-danger"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></button></td>
      </tr>
    {{/each}}
  </tbody>
</table>
